digraph GPT2_Architecture {
	Input [label="Input Tokens" fillcolor=lightblue shape=box style=filled]
	TokenEmb [label="Token Embeddings (768-D)" fillcolor=lightyellow shape=ellipse style=filled]
	PosEmb [label="Positional Embeddings (768-D)" fillcolor=lightyellow shape=ellipse style=filled]
	SumEmb [label="Sum of Token + Positional Embeddings" fillcolor=lightgreen shape=ellipse style=filled]
	Layer1 [label="Transformer Block 1
(Self-Attention + FFN)" fillcolor=lightgray shape=box style=filled]
	Layer2 [label="Transformer Block 2
(Self-Attention + FFN)" fillcolor=lightgray shape=box style=filled]
	Layer3 [label="Transformer Block 3
(Self-Attention + FFN)" fillcolor=lightgray shape=box style=filled]
	Layer4 [label="Transformer Block 4
(Self-Attention + FFN)" fillcolor=lightgray shape=box style=filled]
	Layer5 [label="Transformer Block 5
(Self-Attention + FFN)" fillcolor=lightgray shape=box style=filled]
	Layer6 [label="Transformer Block 6
(Self-Attention + FFN)" fillcolor=lightgray shape=box style=filled]
	Layer7 [label="Transformer Block 7
(Self-Attention + FFN)" fillcolor=lightgray shape=box style=filled]
	Layer8 [label="Transformer Block 8
(Self-Attention + FFN)" fillcolor=lightgray shape=box style=filled]
	Layer9 [label="Transformer Block 9
(Self-Attention + FFN)" fillcolor=lightgray shape=box style=filled]
	Layer10 [label="Transformer Block 10
(Self-Attention + FFN)" fillcolor=lightgray shape=box style=filled]
	Layer11 [label="Transformer Block 11
(Self-Attention + FFN)" fillcolor=lightgray shape=box style=filled]
	Layer12 [label="Transformer Block 12
(Self-Attention + FFN)" fillcolor=lightgray shape=box style=filled]
	LastLayer [label="Final Transformer Layer" fillcolor=orange shape=box style=filled]
	LastEmb [label="Last Layer Embeddings (768-D)" fillcolor=red shape=ellipse style=filled]
	Output [label="Output Logits (Vocab Size)" fillcolor=lightcoral shape=box style=filled]
	Input -> TokenEmb
	Input -> PosEmb
	TokenEmb -> SumEmb
	PosEmb -> SumEmb
	SumEmb -> Layer1
	Layer1 -> Layer2
	Layer2 -> Layer3
	Layer3 -> Layer4
	Layer4 -> Layer5
	Layer5 -> Layer6
	Layer6 -> Layer7
	Layer7 -> Layer8
	Layer8 -> Layer9
	Layer9 -> Layer10
	Layer10 -> Layer11
	Layer11 -> Layer12
	Layer12 -> LastLayer
	LastLayer -> LastEmb
	LastEmb -> Output
}
